
@{
    ViewBag.Title = "ReadyToStart";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model WSimpleCommandExecutor.Models.StartTaskViewModel

<h2>ReadyToStart</h2>
<div class="container">
    
</div>
@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.EditorFor(m => m.Name)
        @Html.ValidationMessageFor(m=>m.Name)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Params)
        @Html.EditorFor(m => m.Params)
        @Html.ValidationMessageFor(m=>m.Params)
    </div>
    <button type="submit" class="btn btn-default">Start task</button>
}